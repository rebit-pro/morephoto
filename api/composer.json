{
    "name": "php/test",
    "description": "test case for php",
    "type": "project",
    "license": "proprietary",
    "version": "1.0.0",
    "require": {
      "php": "^8.4",
      "slim/slim": "^4.0",
      "slim/psr7": "^1.6",
      "uru/digital-river-models": "^1.0",
      "uru/bitrix-mvc": "^1.0",
      "php-di/slim-bridge": "^3.4",
      "ext-json": "*",
      "symfony/console": "*",
        "php-amqplib/php-amqplib": "^3.7",
        "ramsey/uuid": "^4.7",
        "webmozart/assert": "^1.11",
      "ext-intl": "*",
        "predis/predis": "^3.0"
    },
    "require-dev": {
      "roave/security-advisories": "dev-latest",
      "friendsofphp/php-cs-fixer": "^3.75.0",
      "phpunit/phpunit": "^12.0",
      "overtrue/phplint": "^9.5",
      "squizlabs/php_codesniffer": "^3.12",
      "vimeo/psalm": "^6.9",
      "phpstan/phpstan": "^2.1"
    },
    "scripts": {
      "server": "php -S localhost:8000 -t public",
      "fix": "vendor/bin/php-cs-fixer fix src --verbose --allow-risky=yes",
      "app": "php bin/app.php --ansi",
      "lint": "phplint",
      "cs-check": "phpcs --extensions=php",
      "cs-fix": "phpcbf --extensions=php",
      "psalm": "psalm",
      "stan": "vendor/bin/phpstan analyse src --level=6",
      "phpstan": "phpstan analyse --configuration=phpstan.neon",
      "test": "phpunit --colors=always",
      "test-coverage": "phpunit --colors=always --coverage-html var/coverage"
    },
    "autoload": {
      "psr-4": {

      }
    },
    "autoload-dev": {
      "psr-4": {
        "Rebit\\Dev\\": "public/local/modules/rebit.dev/lib/"
      }
    }
}
